                    if (!excludedDays.includes(tempDate.getDay())) {
                        workdaysList.push(tempDate.toDateString())
                        actualtotalWorkdays++
                    }
                }
                var durationInDays = (plannedtotalWorkdays - actualtotalWorkdays);
                durationInDays = durationInDays * -1;
